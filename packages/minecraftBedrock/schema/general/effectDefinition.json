{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"description": "Defines an effect to add to the entity",
	"properties": {
		"effect": {
			"$ref": "./vanilla/effectName.json"
		},
		"duration": {
			"type": "number",
			"description": "The time duration in seconds of the effect"
		},
		"amplifier": {
			"type": "integer",
			"description": "The amplifier for the effect"
		},
		"visible": {
			"type": "boolean",
			"description": "Show particles"
		},
		"display_on_screen_animation": {
			"type": "boolean"
		}
	}
}
