{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Volume",
	"allOf": [
		{
			"properties": {
				"format_version": {
					"type": "string",
					"enum": ["1.17.0", "1.17.30"]
				}
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"const": "1.17.0"
					}
				}
			},
			"then": {
				"$ref": "./v1.17.0/main.json"
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": [
							"1.17.30",
							"1.17.40",
							"1.18.0"
						]
					}
				}
			},
			"then": {
				"$ref": "./v1.17.30/main.json"
			}
		}
	]
}
