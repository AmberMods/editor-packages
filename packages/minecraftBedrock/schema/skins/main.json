{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Skins",
	"properties": {
		"skins": {
			"type": "array",
			"items": {
				"type": "object",
				"additionalProperties": false,
				"properties": {
					"localization_name": {
						"anyOf": [
							{
								"type": "string"
							},
							{
								"type": "integer"
							}
						]
					},
					"geometry": {
						"enum": [
							"geometry.humanoid.custom",
							"geometry.humanoid.customSlim"
						]
					},
					"texture": {
						"type": "string"
					},
					"type": {
						"enum": ["free", "custom"]
					}
				}
			}
		},
		"serialize_name": {
			"type": "string"
		},
		"localization_name": {
			"type": "string"
		}
	}
}
