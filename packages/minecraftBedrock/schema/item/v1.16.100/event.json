{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
		"properties": {
		"trigger": { "$ref": "../../event/v1.16.100/triggerItem.json" },
		"damage": { "$ref": "../../event/v1.16.100/damage.json" },
		"decrement_stack": { "$ref": "../../event/v1.16.100/decrementStack.json" },
		"die": { "$ref": "../../event/v1.16.100/die.json" },
		"add_mob_effect": { "$ref": "../../event/v1.16.100/addMobEffect.json" },
		"remove_mob_effect": { "$ref": "../../event/v1.16.100/removeMobEffect.json" },
		//"play_effect": { "$ref": "../../event/v1.16.100/playEffect.json" },
		//"play_sound": { "$ref": "../../event/v1.16.100/playSound.json" },
		"run_command": { "$ref": "../../event/v1.16.100/runCommand.json" },
		"shoot": { "$ref": "../../event/v1.16.100/shoot.json" },
		"swing": { "$ref": "../../event/v1.16.100/swing.json" },
		"teleport": { "$ref": "../../event/v1.16.100/teleport.json" },
		"transform_item": { "$ref": "../../event/v1.16.100/transformItem.json" },
		"randomize": {
			"type": "array",
			"items": {
				"allOf": [
					{
						"type": "object",
						"properties": {
							"weight": { "type": "number" }
						}
					},
					{
						"$ref": "#"
					}
				]
			}
		},
		"sequence": {
			"type": "array",
			"items": {
				"allOf": [
					{
						"type": "object",
						"properties": {
							"condition": { 
								"type": "string",
								"$ref": "../../molang/embedded.json"
							}
						}
					},
					{
						"$ref": "#"
					}
				]
			}
		} 
	}
}