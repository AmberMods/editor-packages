{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"properties": {
		"add": { "$ref": "../../event/componentGroup/add.json" },
		"remove": { "$ref": "../../event/componentGroup/remove.json" },
		"trigger": { "$ref": "../../event/v1.16.100/triggerEntity.json" },
		"damage": { "$ref": "../../event/v1.16.100/damage.json" },
		"decrement_stack": { "$ref": "../../event/v1.16.200/decrementStack.json" },
		"die": { "$ref": "../../event/v1.16.100/die.json" },
		"add_mob_effect": { "$ref": "../../event/v1.16.100/addMobEffect.json" },
		"remove_mob_effect": { "$ref": "../../event/v1.16.100/removeMobEffect.json" },
		"play_effect": { "$ref": "../../event/v1.16.100/playEffect.json" },
		"play_sound": { "$ref": "../../event/v1.16.100/playSound.json" },
		"run_command": { "$ref": "../../event/v1.16.100/runCommand.json" },
		"teleport": { "$ref": "../../event/v1.16.100/teleport.json" },
		"transform_item": { "$ref": "../../event/v1.16.100/transformItem.json" },
		"set_actor_property": { "$ref": "../../event/v1.17.0/setActorProperty.json" },
		"randomize": {
			"type": "array",
			"items": {
				"allOf": [
					{
						"properties": {
							"weight": { "type": "number" }
						}
					},
					{
						"$ref": "#"
					}
				]
			}
		},
		"sequence": {
			"type": "array",
			"items": {
				"allOf": [
					{
						"properties": {
							"filters": {
								"$ref": "../general/filter.json"
							}
						}
					},
					{
						"$ref": "#"
					}
				]
			}
		} 
	}
}
