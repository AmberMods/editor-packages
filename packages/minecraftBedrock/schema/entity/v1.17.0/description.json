{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"allOf": [
		{
			"$ref": "../description.json"
		},
		{
			"$ref": "../../project/experimentalGameplay/upcomingCreatorFeatures.json",
			"then": {
				"properties": {
					"aliases": {
						"type": "object",
						"patternProperties": {
							".*": {
								"$ref": "../dynamic/currentContext/propertyForAlias.json"
							}
						}
					},
					"properties": {
						"type": "object",
						"patternProperties": {
							".*": {
								"type": "object",
								"properties": {
									"min": {
										"type": "number"
									},
									"max": {
										"type": "number"
									},
									"values": {
										"type": "array",
										"items": {
											"anyOf": [
												{
													"type": "string"
												},
												{
													"type": "number"
												},
												{
													"type": "boolean"
												}
											]
										}
									},
									"default": {
										"type": "number"
									},
									"client_sync": {
										"type": "boolean"
									}
								}
							}
						}
					}
				}
			}
		}
	]
}