{
	"id": "customComponent",
	"type": "text",
	"icon": "mdi-language-javascript",
	"detect": {
		"scope": "BP/components/entity/",
		"matcher": [
			"BP/components/entity/**/*.js",
			"BP/components/entity/**/*.ts"
		]
	},

	"types": [
		"types/customComponent/common.d.ts",
		"types/customComponent/entity.d.ts"
	],

	"documentation": {
		"baseUrl": "https://bridge-core.app/extension-docs/custom-components",
		"supportsQuerying": false
	}
}
