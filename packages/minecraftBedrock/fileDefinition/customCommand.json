{
	"id": "customCommand",
	"type": "text",
	"icon": "mdi-script-text-outline",
	"detect": {
		"scope": "BP/commands/",
		"matcher": ["BP/commands/**/*.js", "BP/commands/**/*.ts"]
	},

	"types": ["types/customCommand/main.d.ts"],

	"documentation": {
		"baseUrl": "https://bridge-core.app/extension-docs/custom-commands/",
		"supportsQuerying": false
	}
}
