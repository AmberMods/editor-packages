{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "targetVersion": {
            "type": "array",
            "minItems": 2,
            "maxItems": 2,
            "items": [
                {
                    "type": "string",
                    "description": "Operator"
                },
                {
                    "type": "string",
                    "description": "Version"
                }
            ]
        },
        "experimentalGameplay": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "packTypes": {
            "type": "array",
            "items": {
                "type": "string",
                "enum": [
                    "worldTemplate",
                    "behaviorPack",
                    "resourcePack",
                    "skinPack"
                ]
            }
        }
    }
}